\begin_body

\begin_layout Title
Entwurf von Anlagen des Schienenverkehrs - Hausaufgabe 2
\end_layout

\begin_layout Author
Jakob Harnisch (371815)
\end_layout

\begin_layout Section*
Aufgabe 1
\end_layout

\begin_layout Subsection*
a) Zweiggleisradien
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{r_{z}}=\frac{1}{r_{0}}-\frac{1}{r_{s}};r_{0}=300m;r_{s}=2800m
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r_{z_{links}}=(\frac{1}{300m}-\frac{1}{2800m})^{-1}=336m
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r_{z_{rechts}}=(\frac{1}{300m}+\frac{1}{2800m})^{-1}=270,9m
\]

\end_inset


\end_layout

\begin_layout Subsection*
b) 
\begin_inset Formula $V_{max,C-D}$
\end_inset

im Kreuzungsbereich
\end_layout

\begin_layout Standard
Überhöhung im Kreuzungsbereich ist zwingend gleich der Überhöhung im Hauptgleis
 
\begin_inset Formula $u_{reg}=6,5$
\end_inset


\begin_inset Formula $\frac{v_{e}\text{²}}{r}=45,5mm$
\end_inset


\begin_inset Foot
status open

\begin_layout Plain Layout
Passen hier die Vorzeichen?
\end_layout

\end_inset

.
 Mit 
\begin_inset Formula 
\[
v=\sqrt{\frac{(u_{reg}+153*p)*r_{z}}{11,8}}
\]

\end_inset

, wobei der kritischste Fall im Bogen nach D (negative Überhöhung, kleinerer
 Radius) angenommen wird, folgt:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
v=\sqrt{\frac{(-45,5+153*0,85)*270,9}{11,8}}=44,05\frac{km}{h}
\]

\end_inset


\end_layout

\begin_layout Subsection*
c) Krümmungsbild
\end_layout

\begin_layout Subsection*
d) Steigern der Geschwindigkeit
\end_layout

\begin_layout Standard
Zum Beispiel, in dem man die Hauptbahn nicht überhöht oder Weichen mit größerem
 Zweiggleisradius verbaut.
\end_layout

\begin_layout Subsection*
e) Seitenbeschleunigung und Ruck berechnen
\end_layout

\begin_layout Standard
\begin_inset Formula $a_{max}=\frac{v\text{²}}{r_{min}}=\frac{(40/3,6)\text{²}m}{270,97s\text{²}}=0,46\frac{m}{s\text{²}}$
\end_inset


\end_layout

\begin_layout Section*
Aufgabe 2
\end_layout

\begin_layout Standard
Bei 
\begin_inset Formula $v_{e}=160km/h$
\end_inset

 ist noch kein Übergangsbogen notwendig.
 Der Mindestradius ergibt sich aus
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r=\frac{11,8*v\text{²}}{u+153*a_{q}};v=160km/h;u=0;a_{q}=0,65m/s\text{²}
\]

\end_inset

zu 
\begin_inset Formula $r=3037,5m$
\end_inset

.
 Daraus folgt für die Länge der gesamtem Gleisverziehung:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
l_{v}=2*\sqrt{r*\triangle e}=2*\sqrt{3037,5m*7,5m}=301,87m
\]

\end_inset


\end_layout

\begin_layout Section*
Aufgabe 3
\end_layout

\begin_layout Subsection*
a) in Deutschland gültige Formeln
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
u_{0}=11,8\frac{v\text{²}}{r}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
regu=6,5\frac{v\text{²}}{r}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
u_{f,max}=130mm
\]

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Section*
Aufgabe 4
\end_layout

\begin_layout Standard
Überhöhungsfehlbetrag 
\begin_inset Formula $u_{f}=u_{0}-u=$
\end_inset


\begin_inset Formula $11,8*\frac{v\text{²}}{r}-150mm=11,8*\frac{(145km/h)\text{²}}{1000m}-150mm=98,1mm$
\end_inset


\end_layout

\begin_layout Standard
Übergangsbogenlänge 
\begin_inset Formula $l_{U}=4*v_{e}*\frac{\triangle u_{f}}{1000}=4*145km/h*\frac{98,1mm}{1000}=56,9m$
\end_inset


\end_layout

\begin_layout Standard
Überhöhungsrampenlänge 
\begin_inset Formula $l_{R}=10*v_{e}*\frac{\triangle u}{1000}=1450km/h*\frac{150mm}{1000}=217,5m$
\end_inset


\end_layout

\begin_layout Standard
Maßgebend daher die Überhöhungsrampenlänge, das Abrückmaß ergibt sich zu
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f=\frac{l_{R}\text{²}}{24*r}=\frac{(217,5m)\text{²}}{24*1000m}=1,97m
\]

\end_inset


\end_layout

